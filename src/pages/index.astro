---
export const prerender = true; // Ensure the page is prerendered for static hosting
import StarlightPage from '@astrojs/starlight/components/StarlightPage.astro';
import { getEntry } from 'astro:content';

const overviewEntry = await getEntry('docs', 'getting_started/overview');
const { Content } = await overviewEntry.render();
---

<StarlightPage frontmatter={overviewEntry.data}>
  <Content />
</StarlightPage>